function MergeChange()
{
	while(true)
	{
		if (theDeltaData.documentElement.childNodes.length<=0) break;
		var node = theDeltaData.documentElement.childNodes[0];
		theDeltaData.documentElement.removeChild(node);
	}
}


function DeltaUpdate(recordNode,FieldName,value,DataName)
{
	if (value==null) return;
	var srcNode=null;
	var changeNode=null;
	var reocordTagName=DataName;
	var seleNodes = theDeltaData.documentElement.selectNodes("//"+reocordTagName);
	for (var i=0;i<seleNodes.length;i++)
	{
		var node = seleNodes[i];
		if (node.getAttribute("UpdateKind")!=null)
		{	
			srcNode = node;
			changeNode = seleNodes[i+1];
			break;		
		}
	}
	if ( srcNode==null )
	{
		var rNode = theDeltaData.createNode(1,reocordTagName,"");
		changeNode = theDeltaData.createNode(1,reocordTagName,"");
		rNode.setAttribute("UpdateKind", "ukModify");
		for ( var i=0;i<recordNode.childNodes.length;i++)
		{
			var fnode=theDeltaData.createNode(1,recordNode.childNodes[i].nodeName,"");
			fnode.text=recordNode.childNodes[i].text;
			rNode.appendChild(fnode);
		}	
		for ( var i=0;i<recordNode.childNodes.length;i++)
		{
			var fnode=theDeltaData.createNode(1,recordNode.childNodes[i].nodeName,"");
			if (fnode.nodeName==FieldName)
			{
				fnode.text=value;
			}
			else
			{
				fnode.text="null";
			}
			changeNode.appendChild(fnode);
		}	
		theDeltaData.documentElement.appendChild(rNode);
		theDeltaData.documentElement.appendChild(changeNode);
	}
	else
	{
		for ( var i=0;i<changeNode.childNodes.length;i++)
		{
			var fnode=changeNode.childNodes[i];
			if (fnode.nodeName==FieldName)
			{
				fnode.text=value;
			}
		}	
	}
}


