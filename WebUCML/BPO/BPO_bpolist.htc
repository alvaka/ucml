function LoadResults(result)
{
    HideMessage();
    if(event.result.error==true)
    {
        var xfaultstring = event.result.errorDetail.string;
        alert(xfaultstring);
        return;
    }

    if(iSingleCallID == event.result.id)
    {
		    var pk = event.result.raw;
        for ( var i=0;i<UseTableList.length;i++)
        {
	          if (theSingleTable==UseTableList[i].TableName)
	          {
		            UseTableList[i].SetDataPacketAndChild(pk);	
		            break;
	          }
        }
    }

    if(iApplyCallID == event.result.id)
    {
        for ( var i=0;i<UseTableList.length;i++)
        {
        if (UseTableList[i].fIDENTITYKey == true)
        {
        fPromptSubmit=false;
        var rows=event.result.value.split("@");
        for(var m=0; m<rows.length; m++)
        {
        var fields=rows[m].split(";");
        if (fields.length==1) break;
        var bco=eval(fields[0]+"Base");
        if (bco.LocateOID_GUID(fields[1])!=null)
        bco.setFieldValueEx(bco.PrimaryKey,fields[2]);
        }
        break;
        }
        }
		    MergeChange();
		    if (fPromptSubmit==true)
		        alert(event.result.value);
        if (flowTask==true)
        {
            var hasRiskPoint = getURLParameters("hasRiskPoint");
            var openBPOName=getURLParameters("BPOName")+"BPO";
            try
            {
                var objTable;
                if (hasRiskPoint && hasRiskPoint == 1) {
                    objTable = eval("window.parent.opener.opener."+openBPOName+".getRootTable()");
                    objTable.Refresh();
                    window.parent.opener.close();
                }
                else {
                    objTable = eval("window.parent.opener."+openBPOName+".getRootTable()");
                    objTable.Refresh();
                }
            }
            catch(e){}
            window.parent.close();
        }
        flowTask=false;
		    AfterSubmit();
		    return;
    }
 
 	  for ( var i=0;i<TaskList.length;i++)
 	  {
 		    var obj = TaskList[i];
 		    if(obj.CallID == event.result.id)
 		    {
 			      obj.DoResult(event.result);
 			      return;
 		    }
 	  }
}

