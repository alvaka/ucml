function CallbuildTreeSubNodes(BCName,parentFieldName,parentOID,CaptionField,node,VCName,mode)
{
    var srv=eval("ServiceHandle."+theBPOName+"Service");
	var headObj = new Object();
	callObj = ServiceHandle.createCallOptions();
	callObj.async = false;
	callObj.params = new Array();
	callObj.params.BCName = BCName;
	callObj.params.parentFieldName = parentFieldName;
	callObj.params.parentOID = parentOID;
	callObj.params.CaptionField = CaptionField;
	callObj.funcName = "buildTreeSubNodes";
	if (mode==1)
	{
	    callObj.funcName = "buildTreeSubNodesWithChild";
	    callObj.params.VCName = VCName;
	}
	callObj.SOAPHeader = new Array();
	callObj.SOAPHeader[0] = headObj;

    var result =srv.callService(callObj);
	if(result.error==true)
	{
		var xfaultstring = result.errorDetail.string;
		alert(xfaultstring);
		return;
	}
    if(result.error==false)
    {
        var xmldoc=result.raw;
        var s=result.value.split("~UCMLSPLIT~");
        if (s.length>1)
        {
            localxml.loadXML(s[1]);
        	  for ( var i=0;i<UseTableList.length;i++)
        	  {
        		    UseTableList[i].AddDataRecords(localxml);
        	   }
        }
        node.setAttribute("TreeNodeSrc",s[0]);
    }
}

