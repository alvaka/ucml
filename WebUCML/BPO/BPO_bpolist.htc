var theCurrentBCLink=null;
var BCLinkWin=null;
function StartBCLink(BCLink)
{
	if (BCLink.BCRunMode!=4)
	{
	if (BCLink.BCRunMode==5)
	{
	eval(BCLink.urlFuncName+"(BCLink)");
	return;
	}
	var theMainPanel=window.document.forms[0];
	var bcObject= theMainPanel.parentElement.all[BCLink.BCName+'BPO'];
	 
	 for ( var i=0;i<UseTableList.length;i++)
	 {
	 		UseTableList[i].NotifyControls("HideSelect");
	 }
	 
	if (bcObject==null)
	{
		var sHTML ='<div id="'+BCLink.BCName+'BPO" style="Z-INDEX: 103;LEFT: 0px; BEHAVIOR: url('+UCMLLocalResourcePath+'BCClient/'+BCLink.BCName+'.htc); WIDTH: 100%; POSITION: absolute; TOP: 0px; HEIGHT: 200px"></div>';
		theMainPanel.parentElement.insertAdjacentHTML("BeforeEnd", sHTML);

		var bcObject= theMainPanel.parentElement.all[BCLink.BCName+'BPO'];
		theCurrentBCLink=BCLink;
		bcObject.moveable=true;;
		bcObject.attachEvent("onreadystatechange", OnBCReady);	//-- attach event to cell --//
	}
	else  {
	    bcObject.Visible=false;
	    bcObject.BCLink=BCLink;
	    bcObject.targetTable = eval(BCLink.targetTable + "Base");
	    bcObject.srcFieldName = BCLink.srcFieldName;
	    bcObject.destFieldName = BCLink.destFieldName;
	    bcObject.Visible=true;
	    bcObject.InitBusinessEnv();
	 }
	}
	else
	{
	window.open(eval(BCLink.urlFuncName+"(BCLink)"),"","location=no,menubar=yes,toolbar=no,status=no,directories=no,scrollbars=yes,resizable=yes");
	}
	
}

function OnBCReady()
{
	var theMainPanel=window.document.forms[0];
	var bcObject= theMainPanel.parentElement.all[theCurrentBCLink.BCName+'BPO'];
	bcObject.onclose = onpopupclose;
	bcObject.targetTable = eval(theCurrentBCLink.targetTable+"Base");
	bcObject.srcFieldName = theCurrentBCLink.srcFieldName;
	bcObject.destFieldName = theCurrentBCLink.destFieldName;
	bcObject.InitApplet(theCurrentBCLink);
}

function onpopupclose()
{
	 for ( var i=0;i<UseTableList.length;i++)
	 {
	 		UseTableList[i].NotifyControls("ShowSelect");
	 }
}
